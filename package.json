{
  "name": "socket_starter",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev-server": "concurrently npm:dev-server:*",
    "dev-server:compile": "rm -rf ./dist/ &&tsc -w",
    "dev-server:run": "nodemon"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/",
      "dist/",
      "coverage/"
    ],
    "watch": [
      "src/server/"
    ],
    "exec": "node --loader ts-node/esm src/server/main.ts --emit",
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json,ts"
  },
  "dependencies": {
    "@adminjs/express": "^4.1.0",
    "@adminjs/mongoose": "^2.0.2",
    "@stripe/stripe-js": "^1.24.0",
    "adminjs": "^5.7.3",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.7",
    "socket.io": "^4.1.2",
    "stripe": "^8.209.0"
  },
  "devDependencies": {
    "@adminjs/upload": "^2.0.2",
    "@babel/preset-typescript": "^7.14.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^15.12.2",
    "@types/socket.io-client": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7",
    "socket.io-client": "^4.1.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
